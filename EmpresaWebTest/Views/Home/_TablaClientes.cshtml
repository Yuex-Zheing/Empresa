@model List<Empresa.Services.Cliente>
    


<table cellpadding="0" cellspacing="0" class="table table-bordered table-condensed">
    <tr>
        <th>Nombres</th>
        <th>Direccion</th>
        <th>Telefono</th>
        <th>Clave</th>
         <th>Estado</th>
         
    </tr>
    @foreach (var p in Model.ToList())
    {
        <tr>
            <td>@p.Nombres</td>
            <td>@p.Direccion</td>
            <td>@p.Telefono</td>
            <td>@p.Clave</td>
            <td>@((p.Estado == "A") ? "ACTIVO" : "INACTIVO")</td>
        </tr>
    }
</table>