@model List<EmpresaWebTest.Models.InfoMovimientos>
    
<table cellpadding="0" cellspacing="0" class="table table-bordered table-condensed">
    <tr>
        <th>Numero Cuenta</th>
        <th>Tipo Cuenta</th>
        <th>Saldo Inicial</th>
        <th>Estado</th>
         <th>Movimiento</th>
    </tr>
    @foreach (var p in Model.ToList())
    {
        <tr>
            <td>@p.NumeroCuenta</td>
            <td>@p.TipoCuenta</td>
            <td>@p.SaldoInicial</td>
            <td>@p.EstadoMovimiento</td>
            <td>@p.Movimiento</td>
        </tr>
    }
</table>