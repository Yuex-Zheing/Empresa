@model List<EmpresaWebTest.Models.InfoCuentas>
    
<table cellpadding="0" cellspacing="0" class="table table-bordered table-condensed">
    <tr>
        <th>Numero de cuenta</th>
        <th>Tipo de Cuenta </th>
        <th>Saldo Inicial</th>
        <th>Estado</th>
         <th>Cliente</th>
    </tr>
    @foreach (var p in Model.ToList())
    {
        <tr>
            <td>@p.NumeroCuenta</td>
            <td>@p.TipoCuenta</td>
            <td>@p.SaldoInicial</td>
            <td>@p.Estado</td>
            <td>@p.Cliente</td>
        </tr>
    }
</table>