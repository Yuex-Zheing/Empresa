@{
    ViewData["Title"] = "Home Page";
}

@model EmpresaWebTest.Models.InfoEmpresa

<h1>Consultas Generales</h1>

<br />
<br />
<h3>Listado de Clientes</h3>
<br />
<button id="btnDlg_CrearCliente" >Nuevo Cliente</button>
<br />

@await Html.PartialAsync("_TablaClientes", Model._Clientes)


<br />
<h3>Listado de Cuentas</h3>
<br />
<button id="btnDlg_CrearCuentas" >Nueva Cuenta</button>
<br />
@await Html.PartialAsync("_TablaCuentas", Model._Cuentas)


<br />
<h3>Listado de Movimientos</h3>

@await Html.PartialAsync("_TablaMovimientos", Model._Movimientos)
<br />
<button id="btnDlg_CrearMovimiento" >Nuevo Movimiento</button>
<br />
<div id="dlgClientes" style="display:none;" >
@await Html.PartialAsync("_MantenimientoClientes")
</div>
